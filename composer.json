{
    "name": "adata-team/laravel-clickhouse-migrations",
    "description": "Clickhouse migrations for Laravel",
    "keywords": [
        "clickhouse",
        "migrations",
        "laravel",
        "package",
        "library"
    ],
    "license": "MIT",
    "homepage": "https://github.com/adata-team/laravel-clickhouse-migrations",
    "authors": [
        {
            "name": "Nickolay Babich",
            "email": "n.babich@adata.kz"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "illuminate/console": "^7.21",
        "illuminate/contracts": "^7.21",
        "illuminate/filesystem": "^7.21",
        "illuminate/support": "^7.21",
        "smi2/phpclickhouse": "^1.3"
    },
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Adata\\ClickhouseMigrations\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adata\\ClickhouseMigrations\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Adata\\ClickhouseMigrations\\Providers\\MigrationProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
